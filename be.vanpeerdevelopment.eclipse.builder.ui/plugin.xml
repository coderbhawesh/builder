<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension point="org.eclipse.ui.commands">
		<command
			id="be.vanpeerdevelopment.eclipse.builder.ui.command.createbuilder" 
			name="Create Builder"
			description="A builder class will be generated for the active class.">
		</command>
	</extension>

	<extension point="org.eclipse.ui.handlers">
    	<handler
   			class="be.vanpeerdevelopment.eclipse.builder.ui.handlers.CreateBuilderHandler"
        	commandId="be.vanpeerdevelopment.eclipse.builder.ui.command.createbuilder">
    	</handler>
	</extension>
	
</plugin>
